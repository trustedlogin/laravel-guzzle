{
    "name": "kozz/laravel-guzzle-provider",
    "description": "Guzzle 5/6 Service Provider for Laravel",
    "keywords": ["laravel", "guzzle", "http"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hongbo Tang",
            "email": "thbourlove@gmail.com"
        },
        {
            "name": "Yury Kozyrev",
            "email": "urakozz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "illuminate/support": "^5.0 | ^6.0| ^7.0 | ^8.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Kozz\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kozz\\Tests\\Laravel\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Kozz\\Laravel\\Providers\\Guzzle"
            ],
            "aliases": {
                "Guzzle": "Kozz\\Laravel\\Facades\\Guzzle"
            }
        }
    }
}
